{
  "$schema": "https://json-schema.org/draft-07/schema",
  "$id": "https://github.com/instill-ai/model-backend/blob/main/config/models/model.json",
  "type": "object",
  "title": "Model",
  "description": "The model data structure",
  "additionalProperties": false,
  "required": [
    "id",
    "model_definition",
    "configuration"
  ],
  "properties": {
    "name": {
      "type": "string",
      "title": "Resource name",
      "description": "The resource name",
      "readOnly": true,
      "ui_order": 0,
      "ui_hidden": true,
      "ui_disabled": true,
      "ui_component": "text"
    },
    "uid": {
      "type": "string",
      "format": "uuid",
      "title": "UUID",
      "description": "The unique UUID",
      "readOnly": true,
      "ui_order": 1,
      "ui_hidden": true,
      "ui_disabled": true,
      "ui_component": "text"
    },
    "id": {
      "type": "string",
      "title": "Name",
      "description": "The model name",
      "pattern": "^[a-z]([a-z0-9-]{0,61}[a-z0-9])?$",
      "minLength": 1,
      "maxLength": 63,
      "ui_order": 2,
      "ui_hidden": true,
      "ui_disabled": true,
      "ui_component": "text",
      "ui_placeholder": "my-model"
    },
    "description": {
      "type": "string",
      "title": "Description",
      "description": "Fill with a short description of your model.",
      "minLength": 0,
      "maxLength": 1023,
      "ui_order": 3,
      "ui_component": "textarea_enable_counter"
    },
    "github_link": {
      "type": "string",
      "title": "Github Link",
      "description": "URL to the model's source code or project repository on GitHub",
      "ui_order": 4,
      "ui_component": "text"
    },
    "link": {
      "type": "string",
      "title": "General Link",
      "description": "A general link associated with the model. This could be to a project page, documentation, or any other relevant URL.",
      "ui_order": 5,
      "ui_component": "text"
    },
    "license": {
      "type": "string",
      "title": "License",
      "description": "The license under which the model is distributed.",
      "minLength": 0,
      "maxLength": 2047,
      "ui_order": 6,
      "ui_component": "textarea_enable_counter"
    },
    "model_definition": {
      "type": "string",
      "title": "Model definition resource name",
      "description": "The resource name of the model definition",
      "ui_order": 7,
      "ui_hidden": true,
      "ui_disabled": true,
      "ui_component": "text"
    },
    "configuration": {
      "type": "object",
      "title": "Configuration",
      "description": "Model configuration JSON that has been validated using the `model_spec` JSON schema of a ModelDefinition",
      "ui_order": 8,
      "ui_disabled": true
    },
    "visibility": {
      "type": "string",
      "title": "Visibility",
      "description": "The model visibility",
      "enum": [
        "VISIBILITY_UNSPECIFIED",
        "VISIBILITY_PRIVATE",
        "VISIBILITY_PUBLIC"
      ],
      "default": "VISIBILITY_UNSPECIFIED",
      "readOnly": true,
      "ui_order": 9,
      "ui_hidden": false,
      "ui_disabled": false,
      "ui_component": "select",
      "ui_enum": [
        "",
        "Private",
        "Public"
      ]
    },
    "region": {
      "type": "string",
      "title": "Region",
      "description": "Specifies the geographical region where the model is located or primarily used.",
      "enum": [
        "UNDETERMINED_REGION",
        "TAIWAN"
      ],
      "default": "UNDETERMINED_REGION",
      "readOnly": false,
      "ui_order": 10,
      "ui_hidden": false,
      "ui_disabled": false,
      "ui_component": "select",
      "ui_enum": [
        "",
        "Taiwan"
      ]
    },
    "hardware_spec": {
      "type": "string",
      "title": "Hardware Spec",
      "description": "The model hardware spec",
      "enum": [
        "CPU",
        "NVIDIA_T4_GPU",
        "NVIDIA_A40_GPU",
        "NVIDIA_A40_LARGE_GPU",
        "NVIDIA_A100_40G_GPU",
        "NVIDIA_A100_80G_GPU"
      ],
      "default": "CPU",
      "readOnly": false,
      "ui_order": 11,
      "ui_hidden": false,
      "ui_disabled": false,
      "ui_component": "select",
      "ui_enum": [
        "CPU",
        "Nvidia T4 GPU",
        "Nvidia A40 GPU",
        "Nvidia A40 (Large) GPU",
        "Nvidia A100 (40GB) GPU",
        "Nvidia A100 (80GB) GPU"
      ]
    },
    "owner_name": {
      "type": "string",
      "description": "The resource name of a user",
      "readOnly": true,
      "ui_order": 12,
      "ui_hidden": true,
      "ui_disabled": true,
      "ui_component": "text"
    },
    "owner": {
      "type": [
        "object",
        "null"
      ],
      "title": "Owner",
      "description": "The resource obeject of a user",
      "additionalProperties": {
        "type": [
          "string",
          "number",
          "boolean",
          "object",
          "null"
        ],
        "description": "Value can be a string, number, boolean, object, or null"
      },
      "readOnly": true,
      "ui_order": 13,
      "ui_hidden": true,
      "ui_disabled": true,
      "ui_component": "text"
    },
    "state": {
      "type": "string",
      "title": "State",
      "description": "The model state",
      "enum": [
        "STATE_UNSPECIFIED",
        "STATE_OFFLINE",
        "STATE_ONLINE",
        "STATE_ERROR"
      ],
      "default": "STATE_UNSPECIFIED",
      "readOnly": true,
      "ui_order": 14,
      "ui_hidden": true,
      "ui_disabled": true,
      "ui_component": "select",
      "ui_enum": [
        "",
        "Offline",
        "Online",
        "Error"
      ]
    },
    "task": {
      "type": "string",
      "title": "Task",
      "description": "The model task",
      "enum": [
        "TASK_UNSPECIFIED",
        "TASK_CLASSIFICATION",
        "TASK_DETECTION",
        "TASK_KEYPOINT",
        "TASK_INSTANCE_SEGMENTATION",
        "TASK_SEMANTIC_SEGMENTATION",
        "TASK_OCR",
        "TASK_TEXT_TO_IMAGE",
        "TASK_IMAGE_TO_IMAGE",
        "TASK_TEXT_GENERATION",
        "TASK_TEXT_GENERATION_CHAT",
        "TASK_VISUAL_QUESTION_ANSWERING"
      ],
      "default": "TASK_UNSPECIFIED",
      "readOnly": true,
      "ui_order": 15,
      "ui_hidden": true,
      "ui_disabled": true,
      "ui_component": "select",
      "ui_enum": [
        "",
        "Image classification",
        "Object detection",
        "Keypoint detection",
        "Instance segmentation",
        "Semantic segmentation",
        "OCR",
        "Text to image",
        "Image to image",
        "Text generation",
        "Text generation chat",
        "Visual Question Answering"
      ]
    },
    "create_time": {
      "type": "string",
      "format": "date-time",
      "title": "Create time",
      "description": "The date when the model is created in the format YYYY-MM-DDTHH:MM::SS.",
      "examples": [
        "2022-04-29T00:20:06.703547Z"
      ],
      "readOnly": true,
      "ui_order": 16,
      "ui_hidden": true,
      "ui_disabled": true,
      "ui_component": "text"
    },
    "update_time": {
      "type": "string",
      "format": "date-time",
      "title": "Update time",
      "description": "The date when the model is updated in the format YYYY-MM-DDTHH:MM::SS.",
      "examples": [
        "2022-04-29T01:33:34.910865Z"
      ],
      "readOnly": true,
      "ui_order": 17,
      "ui_hidden": true,
      "ui_disabled": true,
      "ui_component": "text"
    },
    "delete_time": {
      "type": [
        "string",
        "null"
      ],
      "format": "date-time",
      "title": "Delete time",
      "description": "The date when the model is deleted in the format YYYY-MM-DDTHH:MM::SS.",
      "examples": [
        "2022-04-29T01:33:34.910865Z"
      ],
      "readOnly": true,
      "ui_order": 18,
      "ui_hidden": true,
      "ui_disabled": true,
      "ui_component": "text"
    },
    "version": {
      "type": "string",
      "title": "Version",
      "description": "The latest image version tag (digest)",
      "minLength": 0,
      "maxLength": 255,
      "ui_order": 19,
      "ui_hidden": true,
      "ui_disabled": true,
      "ui_component": "text"
    }
  }
}