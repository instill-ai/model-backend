{
  "$schema": "https://json-schema.org/draft-07/schema",
  "$id": "https://github.com/instill-ai/model-backend/blob/main/config/models/model.json",
  "type": "object",
  "title": "Model",
  "description": "The model data structure",
  "additionalProperties": false,
  "required": [
    "id",
    "model_definition",
    "configuration",
    "visibility",
    "provider",
    "region",
    "hardware"
  ],
  "properties": {
    "name": {
      "type": "string",
      "title": "Resource name",
      "description": "The resource name",
      "readOnly": true,
      "ui_order": 0,
      "ui_hidden": true,
      "ui_disabled": true,
      "ui_component": "text"
    },
    "uid": {
      "type": "string",
      "format": "uuid",
      "title": "UUID",
      "description": "The unique UUID",
      "readOnly": true,
      "ui_order": 1,
      "ui_hidden": true,
      "ui_disabled": true,
      "ui_component": "text"
    },
    "id": {
      "type": "string",
      "title": "Name",
      "description": "The model name",
      "pattern": "^[a-z]([a-z0-9-]{0,61}[a-z0-9])?$",
      "minLength": 1,
      "maxLength": 63,
      "ui_order": 2,
      "ui_hidden": true,
      "ui_disabled": true,
      "ui_component": "text",
      "ui_placeholder": "my-model"
    },
    "description": {
      "type": "string",
      "title": "Description",
      "description": "Fill with a short description of your model.",
      "minLength": 0,
      "maxLength": 1023,
      "ui_order": 3,
      "ui_component": "textarea_enable_counter"
    },
    "model_definition": {
      "type": "string",
      "title": "Model definition resource name",
      "description": "The resource name of the model definition",
      "ui_order": 4,
      "ui_hidden": true,
      "ui_disabled": true,
      "ui_component": "text"
    },
    "configuration": {
      "type": "object",
      "title": "Configuration",
      "description": "Model configuration JSON that has been validated using the `model_spec` JSON schema of a ModelDefinition",
      "ui_order": 5,
      "ui_disabled": true
    },
    "visibility": {
      "type": "string",
      "title": "Visibility",
      "description": "The model visibility",
      "enum": [
        "VISIBILITY_UNSPECIFIED",
        "VISIBILITY_PRIVATE",
        "VISIBILITY_PUBLIC"
      ],
      "default": "VISIBILITY_UNSPECIFIED",
      "ui_order": 6,
      "ui_hidden": true,
      "ui_disabled": true,
      "ui_component": "select",
      "ui_enum": ["", "Private", "Public"]
    },
    "owner_name": {
      "type": "string",
      "description": "The resource name of a user",
      "readOnly": true,
      "ui_order": 7,
      "ui_hidden": true,
      "ui_disabled": true,
      "ui_component": "text"
    },
    "owner": {
      "type": ["object", "null"],
      "title": "Owner",
      "description": "The resource object of a user",
      "additionalProperties": {
        "type": ["string", "number", "boolean", "object", "null"],
        "description": "Value can be a string, number, boolean, object, or null"
      },
      "readOnly": true,
      "ui_order": 7,
      "ui_hidden": true,
      "ui_disabled": true,
      "ui_component": "text"
    },
    "provider": {
      "type": "string",
      "title": "Provider",
      "enum": [
        "PROVIDER_UNSPECIFIED",
        "PROVIDER_GCP",
        "PROVIDER_AWS",
        "PROVIDER_AZURE"
      ],
      "description": "The cloud provider that a model is hosted on",
      "readOnly": true,
      "ui_order": 7,
      "ui_hidden": true,
      "ui_disabled": true,
      "ui_component": "text"
    },
    "region": {
      "type": "string",
      "title": "Region",
      "description": "The region of the cloud provider that a model is hosted on",
      "readOnly": true,
      "ui_order": 7,
      "ui_hidden": true,
      "ui_disabled": true,
      "ui_component": "text"
    },
    "hardware": {
      "type": "string",
      "title": "Region",
      "description": "The region of the cloud provider that a model is hosted on",
      "readOnly": true,
      "ui_order": 7,
      "ui_hidden": true,
      "ui_disabled": true,
      "ui_component": "text"
    },
    "task": {
      "type": "string",
      "title": "Task",
      "description": "The model task",
      "enum": [
        "TASK_UNSPECIFIED",
        "TASK_CLASSIFICATION",
        "TASK_DETECTION",
        "TASK_KEYPOINT",
        "TASK_INSTANCE_SEGMENTATION",
        "TASK_SEMANTIC_SEGMENTATION",
        "TASK_OCR",
        "TASK_TEXT_TO_IMAGE",
        "TASK_IMAGE_TO_IMAGE",
        "TASK_TEXT_GENERATION",
        "TASK_TEXT_GENERATION_CHAT",
        "TASK_VISUAL_QUESTION_ANSWERING"
      ],
      "default": "TASK_UNSPECIFIED",
      "readOnly": true,
      "ui_order": 4,
      "ui_hidden": true,
      "ui_disabled": true,
      "ui_component": "select",
      "ui_enum": [
        "",
        "Image classification",
        "Object detection",
        "Keypoint detection",
        "Instance segmentation",
        "Semantic segmentation",
        "OCR",
        "Text to image",
        "Image to image",
        "Text generation",
        "Text generation chat",
        "Visual Question Answering"
      ]
    },
    "create_time": {
      "type": "string",
      "format": "date-time",
      "title": "Create time",
      "description": "The date when the model is created in the format YYYY-MM-DDTHH:MM::SS.",
      "examples": ["2022-04-29T00:20:06.703547Z"],
      "readOnly": true,
      "ui_order": 8,
      "ui_hidden": true,
      "ui_disabled": true,
      "ui_component": "text"
    },
    "update_time": {
      "type": "string",
      "format": "date-time",
      "title": "Update time",
      "description": "The date when the model is updated in the format YYYY-MM-DDTHH:MM::SS.",
      "examples": ["2022-04-29T01:33:34.910865Z"],
      "readOnly": true,
      "ui_order": 9,
      "ui_hidden": true,
      "ui_disabled": true,
      "ui_component": "text"
    },
    "delete_time": {
      "type": ["string", "null"],
      "format": "date-time",
      "title": "Delete time",
      "description": "The date when the model is deleted in the format YYYY-MM-DDTHH:MM::SS.",
      "examples": ["2022-04-29T01:33:34.910865Z"],
      "readOnly": true,
      "ui_order": 9,
      "ui_hidden": true,
      "ui_disabled": true,
      "ui_component": "text"
    }
  }
}
